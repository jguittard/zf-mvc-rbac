{
  "name": "jguittard/zf-mvc-rbac",
  "description": "Apigility Module to handle role based access controls",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "api",
    "apigility",
    "framework",
    "zf2",
    "rbac"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.zendframework.com/"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.2-dev",
      "dev-develop": "1.2-dev"
    }
  },
  "require": {
    "php": ">=5.3.23",
    "zfcampus/zf-mvc-auth": "^1.1.1"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~3.7",
    "squizlabs/php_codesniffer": "~2.0",
    "zendframework/zend-config": "~2.3",
    "zendframework/zend-loader": "~2.3",
    "zendframework/zend-modulemanager": "~2.3",
    "zendframework/zend-serializer": "~2.3",
    "zendframework/zend-test": "~2.3",
    "zendframework/zend-authentication": "~2.3",
    "zfcampus/zf-deploy": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "ZF\\MvcRbac\\": "src/",
      "ZFTest\\MvcRbac\\": "test/"
    },
    "classmap": [
      "Module.php"
    ]
  }
}